<h1>Faculties#show</h1>
<p><b><%= @faculty.name %></b></p>
<table>
  <tr>
    <th>Name</th>
    <th>Grade</th>
    <th></th>
    <th></th>
  </tr>
<% @student.each do |student| %>
<tr>
    <td><%= student.name %></td>
    <td><%= student.grade %></td>
    <td><%= link_to 'Show', student %></td>
    <td><%= link_to 'Edit', edit_student_path(student) %></td>
    <td><%= link_to 'Destroy', student, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>
<br />
